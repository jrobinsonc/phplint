    "prefer-stable": true,
    "keywords": [
        "phplint",
        "lint",
        "syntax",
        "check"
    ],
    "autoload": {
        "psr-4": {
            "JRdev\\PHPLint\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Jose Robinson",
            "email": "jrobinsonc@gmail.com",
            "homepage": "https://joserobinson.com/"
        }
    ],
    "repositories": [
        {
